<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>ApplicationDetail</servlet-name>
        <servlet-class>controller.ApplicationDetailController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ApplicationListController</servlet-name>
        <servlet-class>controller.ApplicationListController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterAppliDetailController</servlet-name>
        <servlet-class>controller.RegisterAppliDetailController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ApplicationDetail</servlet-name>
        <url-pattern>/applicationdetail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ApplicationListController</servlet-name>
        <url-pattern>/operator/listApplication</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterAppliDetailController</servlet-name>
        <url-pattern>/appli-detail</url-pattern>
    </servlet-mapping>
</web-app>
